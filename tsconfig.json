{
  "compilerOptions": {
    "target": "es6",
    "module": "esnext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "allowJs": true,
  },
  "include": [
    "**/*",
  ],
  "exclude": [
    "node_modules"
  ]
}
